<Window x:Class="EF_Ex3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EF_Ex3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Get table" HorizontalAlignment="Left" Margin="659,226,0,0" VerticalAlignment="Top" Click="Button_Click" Width="121"/>
        <DataGrid x:Name="table" AutoGenerateColumns="True" d:ItemsSource="{Binding products}" Margin="1,0,159,0" />
    </Grid>
</Window>
